{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "indent": [
      "warn",
      2, {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      0,
      "windows"
    ],
    "no-unused-vars": [
      "warn"
    ],
    "semi": [
      "warn",
      "always"
    ],
    "no-console": [
      "off"
    ]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
